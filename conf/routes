GET		/									controllers.Application.index

GET		/login?							controllers.Application.index
GET		/register?						controllers.Application.index
POST		/login/							controllers.Application.login
POST		/register/						controllers.Application.register

GET		/about.html						controllers.Application.about
GET		/contact.html					controllers.Application.contact


GET		/search						controllers.Application.index
GET		/search/						controllers.Application.search


GET     /assets/*file               controllers.Assets.at(path="/public", file)


